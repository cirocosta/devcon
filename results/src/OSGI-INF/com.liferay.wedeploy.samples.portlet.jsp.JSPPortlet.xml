<?xml version="1.0" encoding="UTF-8"?>
<component name="com.liferay.wedeploy.samples.portlet.jsp.JSPPortlet" immediate="true">
  <implementation class="com.liferay.wedeploy.samples.portlet.jsp.JSPPortlet"/>
  <service>
    <provide interface="javax.portlet.Portlet"/>
  </service>
  <property name="javax.portlet.init-param.view-template" type="String" value="/view.jsp"/>
  <property name="javax.portlet.security-role-ref" type="String" value="power-user,user"/>
  <property name="javax.portlet.resource-bundle" type="String" value="content.Language"/>
  <property name="javax.portlet.init-param.template-path" type="String" value="/"/>
  <property name="com.liferay.portlet.header-portlet-css" type="String" value="/css/main.css"/>
  <property name="com.liferay.portlet.instanceable" type="String" value="true"/>
  <property name="javax.portlet.display-name" type="String" value="WeDeploy Portlet"/>
  <property name="com.liferay.portlet.css-class-wrapper" type="String" value="portlet-jsp"/>
  <property name="com.liferay.portlet.display-category" type="String" value="wedeploy.display-category"/>
</component>
